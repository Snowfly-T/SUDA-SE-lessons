{
  "name": "nestjs-lottery-app",
  "version": "0.0.1",
  "author": "Snowflyt",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run prisma:setup && nest start",
    "start:dev": "npm run prisma:setup && nest start --watch",
    "start:debug": "npm run prisma:setup && nest start --debug --watch",
    "start:prod": "npm run prisma:setup && node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "prisma:setup": "ts-node ./prisma && npx prisma generate && npx prisma db push && ts-node ./prisma/setup",
    "prisma:setup-test": "cross-env NODE_ENV=test ts-node ./prisma --test && rimraf ./prisma/test.db && npx prisma db push && ts-node ./prisma/setup-test",
    "test": "npm run prisma:setup-test && jest && rimraf ./prisma/test.db",
    "test:watch": "npm run prisma:setup-test && jest --watch && rimraf ./prisma/test.db",
    "test:cov": "npm run prisma:setup-test && jest --coverage && rimraf ./prisma/test.db",
    "test:debug": "npm run prisma:setup-test && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand && rimraf ./prisma/test.db",
    "test:e2e": "npm run prisma:setup-test && jest --config ./test/jest-e2e.json && rimraf ./prisma/test.db"
  },
  "dependencies": {
    "@apollo/server": "^4.10.1",
    "@nestjs/apollo": "^12.1.0",
    "@nestjs/cache-manager": "^2.2.1",
    "@nestjs/common": "^10.3.3",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.3",
    "@nestjs/graphql": "^12.1.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.3",
    "@prisma/client": "^5.10.2",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.4.0",
    "graphql": "^16.8.1",
    "graphql-intuitive-request": "^0.1.4",
    "graphql-scalars": "^1.22.5",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "ramda": "^0.29.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.3",
    "@types/bcrypt": "^5.0.2",
    "@types/chance": "^1.1.6",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.25",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "chance": "^1.1.11",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-sonarjs": "^0.24.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.12",
    "prisma": "^5.10.2",
    "schemix": "^1.15.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/../src/$1",
      "^@test/(.*)$": "<rootDir>/$1"
    }
  }
}
